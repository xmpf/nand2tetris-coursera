/**
 * Exclusive-or gate:
 * out = not (a == b)
 */

CHIP Xor {
    IN a, b;
    OUT out;

    PARTS:
    Nand(a=a, b=b, out=nand1);
    Nand(a=a, b=nand1, out=nand11);
    Nand(a=nand1, b=b, out=nand12);
    Nand(a=nand11, b=nand12, out=out);
}